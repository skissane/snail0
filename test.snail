proc hello.world {} {
	puts "Hello World!"
}
hello.world

proc square {$x} {
	* $x $x
}
puts [square 8]

proc make.hello.world {$name} {
	proc $name {} {
		puts "Hello World!"
	}
}

make.hello.world hello.world2
hello.world2

proc quux {} {
	set i 0
	loop {
		if {> $i 10} then {break}
		puts $i
		set i [+ $i 1]
	}
}
quux
